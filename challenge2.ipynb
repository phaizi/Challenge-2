    "def phn(a):\n",
    "    num = '('\n",
    "    counter = 0\n",
    "    for i in a:\n",
    "        counter += 1\n",
    "        num += str(i)\n",
    "        if counter == 3:\n",
    "            num += ')'\n",
    "        elif counter == 6:\n",
    "            num += '-'\n",
    "    return num      "
   ]
